[@lua --define long double type
	ctype_longdouble = {
		type = 'c_type',
		c_type = {
			name = function() 
				return 'long double'
			end,
			definition = function() 
				return ''
			end,
			statement = function(varName) 
				return table.concat({
					'long double\tm_', varName, ';\n',
				})
			end,
		},
		delete = {
			definition = function() 
				return ''
			end,
			statement = function(obj) 
				return ''
			end,
		},
		buildVar = function(obj, member) 
			return table.concat({obj,'m_',member})
		end,
		binding = {
			marshall = {
				definition = function()
					return ''
				end,
				statement = function(dst, obj)
					return string.format(
						'_buffer_append(pMarshaller, %s, &%s, sizeof(long double));\n',
						dst,
						obj
					)
				end,
			},
			unmarshall = {
				definition = function() 
					return ''
				end,
				statement = function(src, obj)
					statement = function(varName)
						return string.format(
							'%s = _streamRead(%s, long double);\n',
							obj,
							src
						)
					end
				end,
			},
		},
		test = {
			assign = function()
				return tostring(math.random() * math.random(-2147483647, 2147483647))
			end,
			assert = function(obj1, obj2)
				return string.format("assert(%s == %s);\n", obj1, obj2)
			end
		},
	}
]